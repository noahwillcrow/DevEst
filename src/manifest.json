{
  "manifest_version": 2,
  "name": "Roblox DevEst",
  "short_name": "DevEst",
  "version": "1.1",
  "description": "Estimates USD earnings from any given asset on Roblox.",
  "icons": { "32": "icon.png" },
  "permissions": ["http://*.roblox.com/", "https://*.roblox.com/"],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*.roblox.com/*"],
      "js": ["jquery-3.2.1.min.js", "devest.js"]
    }
  ]
}